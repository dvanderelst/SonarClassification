  issn-linking    = {1530-5627},
  issue           = {11},
  keywords        = {Clinical Competence; Education, Distance, methods; Education, Nursing, methods; Feedback; Humans; Ohio; Patient Simulation; Program Evaluation; Robotics, instrumentation; Simulation Training, methods; Surveys and Questionnaires; e-health; pedagogy; robotics; simulation; telenursing},
  nlm-id          = {100959949},
