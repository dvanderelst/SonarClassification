}

@Article{Paulus2013,
  author        = {Paulus, Markus and Hunnius, Sabine and Bekkering, Harold},
  title         = {{Examining functional mechanisms of imitative learning in infancy: does teleological reasoning affect infants' imitation beyond motor resonance?}},
  journal       = {Journal of experimental child psychology},
  year          = {2013},
  volume        = {116},
  number        = {2},
  pages         = {487--98},
  issn          = {1096-0457},
  __markedentry = {[dieter:6]},
  abstract      = {Recently, researchers have been debating whether infants' selective imitative learning is primarily based on sensorimotor processes (e.g., motor resonance through action perception) or whether inferential processes such as teleological reasoning (i.e., reasoning about the efficiency of others' actions) predominantly explain selective imitation in infancy. The current study directly investigated two different theoretical notions employing the seminal and widely used head touch paradigm. In two conditions, we manipulated whether the action appeared to be efficient while motor resonance was optimized to enhance imitation performance in general. The results showed that infants imitated the target action to the same extent in both conditions irrespective of the action's efficiency. In addition, in both conditions, more infants imitated the head action than in an additional baseline condition or in a condition where the target action was performed by another effector. The results suggest that 14-month-olds do not imitate novel actions according to their apparent efficiency but that motor resonance is an important factor in infants' imitation.},
  doi           = {10.1016/j.jecp.2012.10.009},
  isbn          = {1096-0457 (Electronic)$\backslash$r0022-0965 (Linking)},
  keywords      = {Cognition,Female,Humans,Imitative Behavior,Infant,Infant Behavior,Infant Behavior: psychology,Learning,Male,Psychology, Child,Psychomotor Performance},
  owner         = {dieter},
  pmid          = {23270792},
  publisher     = {Elsevier Inc.},
  timestamp     = {2016.09.15},
  url           = {http://www.sciencedirect.com/science/article/pii/S0022096512001993},
}

@Article{Paulus2011,
  author        = {Paulus, Markus and Hunnius, Sabine and Vissers, Marlies and Bekkering, Harold},
  title         = {{Imitation in infancy: Rational or motor resonance?}},
  journal       = {Child Development},
  year          = {2011},
  volume        = {82},
  number        = {4},
  pages         = {1047--1057},
  issn          = {0009-3920},
  __markedentry = {[dieter:6]},
  abstract      = {The present study investigates the contribution of 2 mechanisms to imitation in infancy. The principle of rational action suggests that infants normatively evaluate the efficiency of observed actions. In contrast, it has been proposed that motor resonance (i.e., the mapping of others' actions onto one's own motor repertoire) plays a central role in imitation. This study tested 14-month-old infants (n = 95) in 5 conditions and manipulated the extent to which the observed actions could be matched onto the infants' own motor repertoire as well as whether the observed behavior appeared to be efficient. The results suggest that motor resonance plays a more central role in imitation in infancy than does a rational evaluation of the observed action.},
  doi           = {10.1111/j.1467-8624.2011.01610.x},
  isbn          = {1467-8624},
  owner         = {dieter},
  pmid          = {21679175},
  timestamp     = {2016.09.15},
}

