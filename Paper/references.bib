@Article{Mueller1988,
  author        = {M{\"{u}}ller, M and Wehner, R},
  title         = {{Path integration in desert ants, Cataglyphis fortis.}},
  journal       = {Proceedings of the National Academy of Sciences of the United States of America},
  year          = {1988},
  volume        = {85},
