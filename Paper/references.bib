  issn-linking    = {1050-9631},
  issue           = {2},
  keywords        = {Animals; Cues; Hippocampus, cytology, physiology; Humans; Mammals, physiology; Orientation, physiology; Space Perception, physiology},
  nlm-id          = {9108167},
