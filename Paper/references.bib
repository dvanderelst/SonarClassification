@Article{Ostchega2000,
  author        = {Ostchega, Y and Harris, T B and Hirsch, R and Parsons, V L and Kington, R},
  title         = {{The prevalence of functional limitations and disability in older persons in the US: data from the National Health and Nutrition Examination Survey III.}},
  journal       = {Journal of the American Geriatrics Society},
  year          = {2000},
  volume        = {48},
  number        = {9},
  pages         = {1132--5},
  issn          = {0002-8614},
  __markedentry = {[dieter:6]},
  abstract      = {OBJECTIVE: To provide estimates by sex and age and by sex and race/ethnicity of the proportion of older Americans who have difficulty with functional limitations and daily activities.$\backslash$n$\backslash$nSETTING: The Third National Health and Nutrition Examination Survey (NHANES III) 1988-1994.$\backslash$n$\backslash$nDESIGN: A cross-sectional nationally representative survey.$\backslash$n$\backslash$nPARTICIPANTS: All persons aged 60 and older who completed a household interview (N = 6,866) during NHANES III (conducted 1988-1994).$\backslash$n$\backslash$nMEASUREMENTS: The self-reported physical and functional disability questions from NHANES III included: lower-extremity function, instrumental activities of daily living, basic activities of daily living, needing help with personal and routine daily activities, and use of assistive devices for walking.$\backslash$n$\backslash$nRESULTS: Non-Hispanic black and Mexican-American men and women generally reported significantly (P {\textless} .01) more disability than did non-Hispanic white men and women. Disability was greater for minority women than for men. For both men and women, the prevalence in disability increased significantly (P {\textless} .01) with age for each measure.$\backslash$n$\backslash$nCONCLUSIONS: These sex-age and sex-race/ethnicity national estimates of disability indicate that minority women may represent a vulnerable subpopulation.},
  isbn          = {0002-8614 (Print)$\backslash$r0002-8614 (Linking)},
  keywords      = {80 and over,Activities of Daily Living,Age Distribution,Aged,Aged: statistics {\&} numerical data,Continental Population Groups,Cross-Sectional Studies,Disabled Persons,Disabled Persons: statistics {\&} numerical data,Female,Geriatric Assessment,Humans,Male,Middle Aged,Needs Assessment,Nutrition Surveys,Population Surveillance,Prevalence,Questionnaires,Sex Distribution,United States},
